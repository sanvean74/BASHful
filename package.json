{
  "name": "bashful",
  "version": "1.0.0",
  "description": "A Terminal Based Dating Simulator",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "eslint .",
    "retest": "npm run lint",
    "jest": "jest --runInBand",
    "test": "npm run jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run jest -- --watchAll",
    "test:verbose": "npm run test -- --verbose",
    "start": "node server.js",
    "start:watch": "nodemon server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "asciify-image": "^0.1.5",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.4.2",
    "chance": "^1.1.3",
    "dotenv": "^8.1.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "inquirer": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "play-sound": "^1.1.3",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.1",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "preset": "@shelf/jest-mongodb",
    "setupFilesAfterEnv": [
      "<rootDir>/e2e/setup.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team-Dead-Ant/BASHful.git"
